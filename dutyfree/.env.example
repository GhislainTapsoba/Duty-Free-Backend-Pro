# .env.example

# Database
DB_NAME=dutyfree_db
DB_USER=postgres
DB_PASSWORD=your_secure_password_here

# Redis
REDIS_PASSWORD=your_redis_password_here

# JWT
JWT_SECRET=your_very_long_and_secure_jwt_secret_key_here_at_least_256_bits

# Application
SPRING_PROFILES_ACTIVE=prod
TZ=Africa/Ouagadougou

# Nginx
NGINX_HOST=dutyfree.example.com

# PgAdmin
PGADMIN_EMAIL=admin@dutyfree.com
PGADMIN_PASSWORD=admin_password_here